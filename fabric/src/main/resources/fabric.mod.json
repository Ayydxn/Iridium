{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${author}"
  ],
  "contact": {
    "homepage": "https://github.com/Ayydxn/Iridium",
    "sources": "https://github.com/Ayydxn/Iridium/issues",
    "issues": "${issue_tracker_url}"
  },
  "license": "${license}",
  "icon": "${icon_path}",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.ayydan.iridium.IridiumClientModFabric"
    ]
  },
  "mixins": [
    "${id}-common.mixins.json",
    "${id}.mixins.json"
  ],
  "depends": {
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "fabric": ">=${fabric_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "yet_another_config_lib_v3": ">=${yacl_version}"
  },
  "custom": {
    "fabric-renderer-api-v1:contains_renderer": true
  }
}